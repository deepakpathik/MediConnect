{
  "expo": {
    "name": "MediConnect",
    "slug": "mediconnect",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Gemini_Generated_Image_iydh3wiydh3wiydh.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "enabled": true,
      "checkAutomatically": "ON_LOAD"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mediconnect.app",
      "infoPlist": {
        "NSCameraUsageDescription": "MediConnect needs camera access to scan appointment cards",
        "NSPhotoLibraryUsageDescription": "MediConnect needs photo library access to select appointment images",
        "NSMotionUsageDescription": "MediConnect needs access to your motion data to track your daily steps"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/Gemini_Generated_Image_iydh3wiydh3wiydh.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.mediconnect.app",
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "INTERNET",
        "ACTIVITY_RECOGNITION"
      ],
      "usesCleartextTraffic": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "2553157c-95cf-4c1d-811d-21023750f8c3"
      }
    },
    "plugins": [
      "expo-asset",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow MediConnect to access your camera to scan appointment cards"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow MediConnect to access your photos to select appointment images"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow MediConnect to use your location to show local health recommendations."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/Gemini_Generated_Image_iydh3wiydh3wiydh.png",
          "color": "#ffffff"
        }
      ]
    ]
  }
}